import java.util.*;
public class newonee {
public static void main(String[] args) {
	Scanner sc=new Scanner(System.in);
	int n=sc.nextInt();
	int a[]=new int[n];
	for(int i=0;i<n;i++) {
		a[i]=sc.nextInt();
	}
	for(int i=0;i<n;i++) {
		
		for(int j=0;j<n;j++) {
			if(i+1==a[j]) {
				System.out.print(j+1+" ");
			}
		}
	}
	System.out.println();
}
}
