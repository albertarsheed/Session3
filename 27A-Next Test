import java.io.*;
import java.util.*;

public class Queueproblem {

	public static void main(String[] args){
		       Scanner sc=new Scanner(System.in);
		       TreeSet<Integer> in=new TreeSet();
		       for(int i=1;i<=3001;i++) {
		    	   in.add(i);
		       }
		       int n=sc.nextInt();
		       int a[]=new int[n];
		       for(int i=0;i<n;i++) {
		    	   a[i]=sc.nextInt();
		    	   in.remove(a[i]);
		       }
		       System.out.println(in.first());
		     
	}

}
